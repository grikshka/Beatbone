   <children>
      <ListView fx:id="lstGenres" layoutX="25.0" layoutY="14.0" onMouseClicked="#clickOnGenres" prefHeight="200.0" prefWidth="200.0" />
      <Button layoutX="45.0" layoutY="221.0" mnemonicParsing="false" onAction="#clickCancel" text="Cancel" />
      <Button fx:id="btnSave" layoutX="159.0" layoutY="221.0" mnemonicParsing="false" onAction="#clickSaveGenre" text="Save" />
   </children>
</AnchorPane>
