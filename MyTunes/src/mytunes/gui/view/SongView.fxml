<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="324.0" prefWidth="463.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mytunes.gui.controller.SongViewController">
   <children>
      <Label layoutX="56.0" layoutY="48.0" text="Title" />
      <Label layoutX="53.0" layoutY="83.0" text="Artist" />
      <Label layoutX="51.0" layoutY="124.0" text="Genre" />
      <Label layoutX="54.0" layoutY="167.0" text="Time" />
      <Label layoutX="58.0" layoutY="214.0" text="File" />
      <TextField fx:id="txtTitle" layoutX="101.0" layoutY="44.0" onKeyReleased="#keyTitleTyped" prefHeight="25.0" prefWidth="246.0" />
      <TextField fx:id="txtArtist" layoutX="101.0" layoutY="79.0" onKeyReleased="#keyArtistTyped" prefHeight="25.0" prefWidth="246.0" />
      <TextField fx:id="txtTime" layoutX="101.0" layoutY="163.0" prefHeight="25.0" prefWidth="80.0" />
      <TextField fx:id="txtFile" layoutX="101.0" layoutY="210.0" prefHeight="25.0" prefWidth="246.0" />
      <ComboBox fx:id="cmbGenre" layoutX="101.0" layoutY="120.0" onAction="#clickGenrePicked" prefHeight="25.0" prefWidth="159.0" />
      <Button layoutX="268.0" layoutY="120.0" mnemonicParsing="false" onAction="#clickAddNewGenre" text="More..." />
      <Button fx:id="btnSave" layoutX="382.0" layoutY="263.0" mnemonicParsing="false" onAction="#clickSave" text="Save" />
      <Button fx:id="btnChoosePath" layoutX="359.0" layoutY="210.0" mnemonicParsing="false" onAction="#clickChooseFilePath" text="Choose..." />
      <Button layoutX="321.0" layoutY="263.0" mnemonicParsing="false" onAction="#clickCancel" text="Cancel" />
   </children>
</AnchorPane>
